<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.stockingfun">

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/thestocking"
        android:label="Stocking Stuffer"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name="com.example.stockingstuffer.gameplay.Scores"></activity>
        <activity android:name="com.example.stockingstuffer.gameplay.theGame" />

        <service
            android:name="com.example.stockingstuffer.helpers.MusicService"
            android:enabled="true" />

        <activity android:name="com.example.stockingstuffer.helpers.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>